{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "array-type": [true, "array"],
        "semicolon": [true, "never"],
        "no-console": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-unused-expression": false,
        "radix": false
    },
    "rulesDirectory": []
}